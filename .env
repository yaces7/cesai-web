# Development ortamı için
REACT_APP_API_URL=http://localhost:8000
REACT_APP_CORS_ENABLED=true
REACT_APP_DEBUG=true
REACT_APP_ENVIRONMENT=development

# Production ortamı için (backend'i deploy ettikten sonra güncellenecek)
# REACT_APP_API_URL=

REACT_APP_FIREBASE_API_KEY=
REACT_APP_FIREBASE_AUTH_DOMAIN=
REACT_APP_FIREBASE_PROJECT_ID=
REACT_APP_FIREBASE_STORAGE_BUCKET=
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=
REACT_APP_FIREBASE_APP_ID=
REACT_APP_FIREBASE_MEASUREMENT_ID=

# Yapay Zeka API erişim anahtarları
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_AI_FEEDBACK_ENABLED=true

# Uygulama ayarları
REACT_APP_MAX_MESSAGE_LENGTH=3000
REACT_APP_MESSAGE_COMPRESSION_ENABLED=true
REACT_APP_DEFAULT_LANGUAGE=tr

# Gelişmiş özellikler
REACT_APP_ENABLE_VOICE_INPUT=false
REACT_APP_ENABLE_IMAGE_ANALYSIS=false
